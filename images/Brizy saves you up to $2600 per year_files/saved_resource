function brz_cart_rc(data,cs) {
var xhr = new XMLHttpRequest();xhr.open('POST', 'https://www.brizy.io/account/bzcart/', true); xhr.setRequestHeader('Accept', 'application/json');xhr.setRequestHeader('Content-Type', 'application/json');
//xhr.send(JSON.stringify({pid:data.eventData.product.id,prn:data.eventData.product.name,em:data.eventData.user.email,cs:cs,ev:data.event,
//value:data.eventData.checkout.prices.customer.total,tax:data.eventData.checkout.prices.customer.total_tax,currency:data.eventData.checkout.prices.customer.currency}));
if (bzuname) {
    data.eventData.bzuname = bzuname;
}
xhr.send(JSON.stringify({evn:data.event,cs:cs,evndata:data.eventData}));
}
function brz_forHumans(seconds) {
    var levels = [
        [Math.floor(seconds / 31536000), 'y'],
        [Math.floor((seconds % 31536000) / 86400), 'd'],
        [Math.floor(((seconds % 31536000) % 86400) / 3600), 'h'],
        [Math.floor((((seconds % 31536000) % 86400) % 3600) / 60), 'm'],
        [(((seconds % 31536000) % 86400) % 3600) % 60, 's'],
    ];
    var returntext = '';
    for (var i = 0, max = levels.length; i < max; i++) {
        if (levels[i][0] === 0 && levels[i][1] != 's') continue;
        returntext +=  levels[i][0] + levels[i][1];
    }
    return returntext.trim();
}

