        '                   </div>',
        '               </div>',
        '               <div id="marketingConsentWrapper" style="display:flex;justify-content:flex-end;-webkit-box-pack:end;margin-bottom:1.9em">',
        '                   <div width="100%" style="background-color:transparent;border:none rgb(235, 236, 240);border-radius:4px;height:auto;padding:0px;text-align:left;width:100%;">',
        '                       <div style="display:flex;">',
        '                           <div style="margin-top:0px;">',
        '                               <input type="checkbox" name="marketingConsent" style="height:0px;left:0px;margin:0px;position:absolute;top:0px;width:0px;box-sizing:border-box;line-height:1.15;" value="1"/>',
        '                               <span id="marketingConsent" height="1.2em" width="1.2em" style="border:1px solid rgb(157, 157, 157);border-radius:2px;box-sizing:border-box;box-shadow:none;display:inline-block;height:1.2em;width:1.2em;cursor:pointer;">',
        '                                   <svg fill="none" height="1.2em" viewBox="1 1.25 11 11" width="1.2em" xmlns="http://www.w3.org/2000/svg" style="position:absolute;"><path fill-rule="evenodd" clip-rule="evenodd" d="M9.71401 4.70022L5.08389 9.42113L1.79297 6.1302L3.20718 4.71598L5.0701 6.5789L8.28613 3.2998L9.71401 4.70022Z" fill="#FFFFFF"></path></svg>',
        '                               </span>',
        '                           </div>',
        '                           <div>',
        '                               <label style="padding-left:1em;font-size:1.5em;font-weight:400;white-space:normal;display:inline-block;color: rgb(85, 85, 85);">',
        '                                   <var style="font-style:italic;">ProWebCraft LTD</var> may send me product updates and offers via email. It is possible to opt-out at any time.',
        '                               </label>',
        '                           </div>',
        '                       </div>',
        '                   </div>',
        '               </div>',
        '               <div style="text-align:center;">',
        '                   <div style="font-size:10px">',
        '                       <button type="submit" style="background-color:rgb(5, 179, 230);border-color:rgb(5, 179, 230);border-style:solid;border-radius:4px;border-width:1px;color:rgb(255, 255, 255);cursor:pointer;box-sizing:border-box;box-shadow:none;line-height:20px;height:44px;margin-bottom:auto;min-height:auto;padding:0px;font-size:1.7em;text-align:center;font-weight:700;font-family:inherit;width:100%;background-position:center center !important;">Continue »</button>',
        '                   </div>',
        '               </div>',
        '           </form>',
        '       </div>',
        '   </div>',
        '   </div>',
        '</div>',
        '<div id="bz-modal-js-overlay"></div>',
    ].join("\n");
    var bzpopup = document.createElement('div');
    bzpopup.id = 'bz-popup-modal';
    bzpopup.innerHTML = html;
    document.body.appendChild(bzpopup);

    var bzopencart = document.getElementById('bzopencart');
    var marketingConsentWrapper = document.getElementById('marketingConsentWrapper');
    var marketingConsent = document.getElementById('marketingConsent');

    var pid = paddle_buy_button.getAttribute('href').substring(1);
    var cat = brz_pr_category(pid);
    var bzcartitemname = document.getElementById('bz-cart-item-name');
    bzcartitemname.innerHTML = cat.item_name;

    marketingConsentWrapper.addEventListener('click', function(){
        var checkboxes = document.getElementsByName('marketingConsent');
        for (var checkbox of checkboxes) {
            checkbox.checked = !checkbox.checked;
            if (checkbox.checked) {
                marketingConsent.classList.add('on');
            } else {
                marketingConsent.classList.remove('on');
            }
        }
    });

    bzopencart.addEventListener('submit', function(evn){
        evn.preventDefault();
        let email = bzopencart.elements['email'].value;
        let marketingConsent = bzopencart.elements['marketingConsent'].checked;
        marketingConsent = (marketingConsent) ? '1' : '0';
        bzuname = bzopencart.elements['bzuname'].value;
        openBrizyPaddleCheckout(e,paddle_buy_button,email,marketingConsent);
    });
}

function openBrizyPaddleCheckout(e,paddle_buy_button,email,marketingConsent) {
    e.preventDefault();
    var urlParams = new URLSearchParams(window.location.search);
    var bzprc = urlParams.get('bzprc');
    var pid = paddle_buy_button.getAttribute('href').substring(1);
    var pid = pid.split('/checkout/')[1] ? pid.split('/checkout/')[1] : pid;
    var bzcoupon = null;
    if (pid.split('coupon=')[1]) {bzcoupon = pid.split('?coupon=')[1];pid = pid.split('?coupon=')[0];}
    if (bzcoupon === null && bzprc !=null) {bzcoupon = bzprc;}
    var bzpassthrough = {};
            bzpassthrough.brizypaddlekey = 'MTA1LjU0LjE1NC4xNzMtVVNELXsiNjg4Mjc1Ijo0OSwiNjg4Mjk1Ijo5OSwiNjg4MzAyIjoxOTksIjY4ODMwOSI6NzksIjY4ODMxMyI6Mjk5LCI2ODgzMTYiOjQ5OSwiNjg4MzE5Ijo5LCI2ODgzMjIiOjI5LCI2ODgzMjYiOjQ5LCI2ODgzMzEiOjE0OSwiNjg4MzM2IjoyOTksIjY4ODMzOSI6Mzk5LCI3NjQ2MjYiOjE3OSwiNzY0NjI4Ijo2NzQsIjc2NDYyOSI6MTM0OX0tS0U=';
    var country = 'KE';
    var bzprams = JSON.stringify(bzpassthrough); console.log(bzprams);
    var doupsell = {};

    
    //let modal_overlay = document.getElementById('bz-modal-js-overlay');
    //if (modal_overlay) {modal_overlay.remove();}

    let bzpopupmodal = document.getElementById('bz-popup-modal');
    if (bzpopupmodal) {bzpopupmodal.remove();}

    if (pid == 688275) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
    } else if (pid == 688295) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
    } else if (pid == 688302) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
    } else if (pid == 688331) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
    } else if (pid == 688336) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
    } else if (pid == 688339) {
        //Paddle.Checkout.open({product:pid,quantity_variable:0,allowQuantity:0,override:'https://checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
    } else if (pid == 688309) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams, ...doupsell});
    } else if (pid == 688313) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams, ...doupsell});
    } else if (pid == 688316) {
        //Paddle.Checkout.open({product:pid,quantity_variable:0,allowQuantity:0,success:'https://www.brizy.io/account/thanks',override:'https://checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams, ...doupsell});
    } else if (pid == 688319) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams, ...doupsell});
    } else if (pid == 688322) {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams, ...doupsell});
    } else if (pid == 688326) {
        //Paddle.Checkout.open({product:pid,quantity_variable:0,allowQuantity:0,success:'https://www.brizy.io/account/thanks',override:'https://checkout.paddle.',coupon:bzcoupon,passthrough:bzprams});
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,override:'https://create-checkout.paddle.',coupon:bzcoupon,passthrough:bzprams, ...doupsell});
    } else {
        Paddle.Checkout.open({product:pid,email:email,country:country,marketingConsent:marketingConsent,quantity_variable:0,allowQuantity:0,coupon:bzcoupon,passthrough:bzprams, ...doupsell});
    }

}

function brz_pr_category(pid) {
    pid = String(pid);
    if (pid.split('coupon=')[1]) {pid = pid.split('?coupon=')[0];}
    var cat = new Object();
    var item_name = new Object();
    switch (pid) {
        case '688275':
            item_name['688275'] = 'Brizy Personal WordPress';
        case '688295':
            item_name['688295'] = 'Brizy Freelancer WordPress';
        case '688302':
            item_name['688302'] = 'Brizy Agency WordPress';
            cat['item_list_id'] = 'wordpress_yearly';
            cat['item_list_name'] = 'WordPress Yearly';
            cat['item_category'] = 'WordPress';
            cat['item_category2'] = 'Yearly';
            break;
        case '688331':
            item_name['688331'] = 'Brizy Personal WordPress';
        case '688336':
            item_name['688336'] = 'Brizy Freelancer WordPress';
        case '688339':
            item_name['688339'] = 'Brizy Agency WordPress';
            cat['item_list_id'] = 'wordpress_ltd';
            cat['item_list_name'] = 'WordPress LTD';
            cat['item_category'] = 'WordPress';
            cat['item_category2'] = 'LTD';
            break;
        case '688319':
            item_name['688319'] = 'Brizy Personal Cloud';
        case '688322':
            item_name['688322'] = 'Brizy Freelancer Cloud';
        case '688326':
            item_name['688326'] = 'Brizy Agency Cloud';
            cat['item_list_id'] = 'cloud_monthly';
            cat['item_list_name'] = 'Cloud Monthly';
            cat['item_category'] = 'Cloud';
            cat['item_category2'] = 'Monthly';
            break;
        case '688309':
            item_name['688309'] = 'Brizy Personal Cloud';
        case '688313':
            item_name['688313'] = 'Brizy Freelancer Cloud';
        case '688316':
            item_name['688316'] = 'Brizy Agency Cloud';
            cat['item_list_id'] = 'cloud_yearly';
            cat['item_list_name'] = 'Cloud Yearly';
            cat['item_category'] = 'Cloud';
            cat['item_category2'] = 'Yearly';
            break;
        case '764626':
            item_name['764626'] = 'Personal Cloud LTD';
        case '764628':
            item_name['764628'] = 'Freelancer Cloud LTD';
        case '764629':
            item_name['764629'] = 'Agency Cloud LTD';
            cat['item_list_id'] = 'cloud_ltd';
            cat['item_list_name'] = 'Cloud LTD';
            cat['item_category'] = 'Cloud';
            cat['item_category2'] = 'LTD';
            break;
            default:
                cat['item_list_id'] = 'wordpress_yearly';
                item_name[pid] = 'Brizy Pro';
    }
    cat['item_name'] = item_name[pid];
    return cat;
}

