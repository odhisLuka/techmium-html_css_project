function brzAddToCart() {
    var paddle_buy_buttons = document.querySelectorAll('.paddle_buy_button');
    var item_list_id;
    paddle_buy_buttons.forEach(function(button) {
        var bounding = button.getBoundingClientRect();
        var pid = button.getAttribute('href').substring(1);
        if (bounding.top > 0 && bounding.left > 0 && bounding.right > 0 && bounding.bottom > 0) {
            var newHandler = function(e) { brizyOpenCart(e, this); e.stopImmediatePropagation(); };
            button.addEventListener('click', newHandler, false);
            var cat = brz_pr_category(pid);
            item_list_id = cat.item_list_id;
        }
    });
    if (item_list_id) {
        window[item_list_id]();
    }
}

brzAddToCart();
document.querySelectorAll('.brz-switcher__nav').forEach(function(switcher) {
    switcher.addEventListener('click', function(e) { brzAddToCart(); }, false);
});

function bsetCookie(cname, cvalue, exdays) {
    const d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    let expires = "expires="+ d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function checkoutComplete(data) {
    var bzcheckout = btoa(JSON.stringify(data));
    bsetCookie('bzcheckout',bzcheckout,1);
    setTimeout(function(){
        window.location.href='https://www.brizy.io/account/thanks?bzcheckout='+bzcheckout;
    }, 3*1000);
}

