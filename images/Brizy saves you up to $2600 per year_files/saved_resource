(function(w,d,s){var f=d.getElementsByTagName(s)[0],j=d.createElement(s);
    j.src='https://cdn.paddle.com/paddle/paddle.js';
    j.onreadystatechange = PaddleCallback;j.onload = PaddleCallback;
    f.parentNode.insertBefore(j,f);
})(window,document,'script');
